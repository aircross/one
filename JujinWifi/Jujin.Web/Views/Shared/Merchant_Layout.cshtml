<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="@Url.Content("~/js/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <!-- bootstrap -->
    <link href="/css/bootstrap.custom.min.css" type="text/css" rel="stylesheet" />
    <!-- libraries -->
    <link href="/css/font-awesome.min.css" type="text/css" rel="stylesheet" />
    <!-- global styles -->
    <link rel="stylesheet" type="text/css" href="/css/pages.css"  />
    <!-- font option -->
    <!--[if lt IE 9]>
      <script src="/js/html5.js"></script>
    <![endif]-->
    <!-- scripts -->
    <script src="/js/bootstrap.min.js"></script>
	<script src="/js/theme.js"></script>
    <script src="/js/My97DatePicker/WdatePicker.js" type="text/javascript"></script>
    <!-- this page specific styles -->
    <link rel="stylesheet" href="/css/signin.css" type="text/css" media="screen" />
    <link media="screen" type="text/css" href="/css/index.css" rel="stylesheet">
</head>

<body>
    @RenderBody()
        @{
        string msg = Jujin.Web.JujinContext.GetMsg();
      }
    @if (msg!=null&&msg != "")
    {
    <script type="text/javascript"> 
        $(document).ready(function () {
            var msg = @Html.Raw(msg);
            if (msg.msg!="") {
                alert(msg.msg);
            }
        });
    </script>
    }
</body>
</html>
